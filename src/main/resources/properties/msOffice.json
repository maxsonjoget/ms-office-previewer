[
   {
      "title":"@@form.OfficeViewerElement.title@@",
      "properties":[
         {
            "name":"id",
            "label":"@@form.OfficeViewerElement.id@@",
            "type":"textfield",
            "required":"True",
            "regex_validation":"^[a-zA-Z0-9_]+$",
            "validation_message":"@@form.OfficeViewerElement.invalidId@@",
            "description" : "@@form.OfficeViewerElement.id.desc@@"
         },
         {
            "name":"label",
            "label":"@@form.OfficeViewerElement.label@@",
            "type":"textfield",
            "required":"True",
            "value":"Office Viewer"
         }
      ]
   },
   {
      "title":"@@form.OfficeViewerElement.advancedOptions@@",
      "properties":[
         {
            "label":"@@form.OfficeViewerElement.data@@",
            "type":"header"
         },
         {
            "name":"validator",
            "label":"@@form.OfficeViewerElement.validator@@",
            "type":"elementselect",
            "options_ajax":"[CONTEXT_PATH]/web/property/json/getElements?classname=org.joget.apps.form.model.FormValidator",
            "url":"[CONTEXT_PATH]/web/property/json[APP_PATH]/getPropertyOptions"
         },
         {
            "name":"maxSize",
            "label":"@@form.OfficeViewerElement.maxSize@@",
            "type":"textfield",
            "regex_validation":"^[0-9]+$",
            "validation_message":"@@form.OfficeViewerElement.invalidSize@@"
         },
         {
            "name":"maxSizeMsg",
            "label":"@@form.OfficeViewerElement.maxSize.msg@@",
            "type":"textfield",
            "value":"@@form.OfficeViewerElement.maxSize.msg.fileTooBig@@"
         },
         {
            "name":"fileType",
            "label":"@@form.OfficeViewerElement.fileType@@",
            "type":"textfield"
         },
         {
            "name":"fileTypeMsg",
            "label":"@@form.OfficeViewerElement.fileType.msg@@",
            "type":"textfield",
            "value":"@@form.OfficeViewerElement.fileType.msg.invalidFileType@@"
         },
         {
            "name":"removeFile",
            "label":"@@form.OfficeViewerElement.removeFile@@",
            "type":"checkbox",
            "value":"false",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "label":"@@form.OfficeViewerElement.resize@@",
            "type":"header"
         },
         {
            "name":"resizeWidth",
            "label":"@@form.OfficeViewerElement.resizeWidth@@",
            "type":"textfield"
         },
         {
            "name":"resizeHeight",
            "label":"@@form.OfficeViewerElement.resizeHeight@@",
            "type":"textfield"
         },
         {
            "name":"resizeQuality",
            "label":"@@form.OfficeViewerElement.resizeQuality@@",
            "type":"textfield",
            "value":"0.8"
         },
         {
            "name":"resizeMethod",
            "label":"@@form.OfficeViewerElement.resizeMethod@@",
            "type":"selectbox",
            "value":"",
            "options":[
               {
                  "value":"",
                  "label":"@@form.OfficeViewerElement.contain@@"
               },
               {
                  "value":"crop",
                    "label":"@@form.OfficeViewerElement.crop@@"
               }
            ]
         },
         {
            "label":"@@form.section.permission@@",
            "type":"header"
         },
         {
            "name":"permissionType",
            "label":"@@form.section.permission@@",
            "type":"selectbox",
            "value":"",
            "options":[
               {
                  "value":"",
                  "label":"@@form.OfficeViewerElement.permission.loggedInUsers@@"
               },
               {
                  "value":"public",
                  "label":"@@form.OfficeViewerElement.permission.public@@"
               },
               {
                  "value":"custom",
                  "label":"@@form.OfficeViewerElement.permission.custom@@"
               }
            ]
         },
         {
            "name":"permissionPlugin",
            "label":"@@form.OfficeViewerElement.customPermission@@",
            "type":"elementselect",
            "control_field":"permissionType",
            "control_value":"custom",
            "control_use_regex":"false",
            "required":"true",
            "options_ajax":"[CONTEXT_PATH]/web/property/json/getElements?classname=org.joget.apps.form.model.FormPermission",
            "url":"[CONTEXT_PATH]/web/property/json[APP_PATH]/getPropertyOptions"
         },
         {
            "label":"@@form.OfficeViewerElement.ui@@",
            "type":"header"
         },
         {
            "name":"size",
            "label":"@@form.OfficeViewerElement.size@@",
            "type":"textfield"
         },
         {
            "name":"multiple",
            "label":"@@form.OfficeViewerElement.multiple@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "name":"readonly",
            "label":"@@form.OfficeViewerElement.readonly@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "name":"padding",
            "label":"@@form.OfficeViewerElement.dropzonePadding@@",
            "type":"textfield"
         },
         {
            "name":"attachment",
            "label":"@@form.OfficeViewerElement.download@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         }
      ]
   }
]